{
    "name": "loopback-component-crud",
    "version": "0.5.0",
    "description": "Loopback component generic CRUD application",
    "keywords": [
        "loopback-extension",
        "loopback"
    ],
    "main": "index.js",
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "clean": "lb-clean dist *.tsbuildinfo",
        "prebuild": "npm run clean",
        "build": "lb-tsc --outDir dist",
        "build:watch": "lb-tsc --watch --outDir dist",
        "start": "node .",
        "start:watch": "nodemon .",
        "test": "lb-mocha --allow-console-logs \"test/**/*.test.ts\"",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/loopback4/loopback-component-crud"
    },
    "author": "KoLiBer",
    "license": "MIT",
    "files": [
        "README.md",
        "index.js",
        "index.d.ts",
        "dist"
    ],
    "dependencies": {
        "@loopback/authentication": "^4.2.3",
        "@loopback/authorization": "^0.5.8",
        "@loopback/boot": "^2.2.0",
        "@loopback/context": "^3.7.0",
        "@loopback/core": "^2.5.0",
        "@loopback/repository": "^2.4.0",
        "@loopback/rest": "^4.0.0",
        "@loopback/rest-explorer": "^2.2.0",
        "@loopback/security": "^0.2.8",
        "apollo-server": "^2.12.0",
        "graphql": "^15.0.0",
        "openapi-to-graphql": "^2.1.0",
        "tslib": "^1.10.0"
    },
    "devDependencies": {
        "@loopback/build": "^5.3.1",
        "@loopback/testlab": "^3.1.3",
        "@types/node": "^10.17.21",
        "nodemon": "^2.0.3",
        "source-map-support": "^0.5.19",
        "typescript": "~3.8.3"
    }
}
