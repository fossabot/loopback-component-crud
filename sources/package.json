{
    "name": "loopback-component-crud",
    "version": "0.8.1",
    "description": "Loopback component generic CRUD application",
    "keywords": [
        "loopback-extension",
        "loopback"
    ],
    "main": "index.js",
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "clean": "lb-clean dist *.tsbuildinfo",
        "prebuild": "npm run clean",
        "build": "lb-tsc --outDir dist",
        "build:watch": "lb-tsc --watch --outDir dist",
        "start": "node .",
        "start:watch": "nodemon .",
        "test": "lb-mocha --allow-console-logs \"test/**/*.test.ts\"",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/loopback4/loopback-component-crud"
    },
    "author": "KoLiBer",
    "license": "MIT",
    "files": [
        "README.md",
        "index.js",
        "index.d.ts",
        "dist"
    ],
    "dependencies": {
        "@loopback/authentication": "^4.2.6",
        "@loopback/authorization": "^0.5.11",
        "@loopback/boot": "^2.3.2",
        "@loopback/context": "^3.8.2",
        "@loopback/core": "^2.7.1",
        "@loopback/repository": "^2.6.0",
        "@loopback/rest": "^5.1.0",
        "@loopback/rest-explorer": "^2.2.3",
        "@loopback/security": "^0.2.11",
        "apollo-server": "^2.14.1",
        "graphql": "^15.0.0",
        "openapi-to-graphql": "^2.1.0",
        "tslib": "^2.0.0"
    },
    "devDependencies": {
        "@loopback/build": "^5.4.2",
        "@loopback/testlab": "^3.1.6",
        "@types/node": "^10.17.24",
        "nodemon": "^2.0.4",
        "ts-node": "^8.10.2",
        "typescript": "~3.9.3"
    }
}
